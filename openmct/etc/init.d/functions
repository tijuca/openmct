success() {
 echo ' [  OK  ]'
 checkVT102
}

failure() {
 echo ' [FAILED]'
 checkVT102
}

skipped() {
 echo ' [ SKIP ]'
 checkVT102
}

checkVT102() {
 [ "${TERM}" == "vt102" ] && echo -en "\r"
}

_status() {
 ps=$(ps|grep ${exe}|grep -v grep)
 if [ "$ps" == "" ]; then
  echo "$(basename $0) ($(basename ${exe})) not running."
 else
  echo "$(basename $0) ($(basename ${exe})) running."
 fi
}
